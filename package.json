{
  "name": "tyte",
  "description": "Write Jest tests for TypeScript types",
  "version": "0.2.0",
  "license": "MIT",
  "keywords": [
    "jest",
    "unit",
    "test",
    "tests",
    "typescript",
    "type",
    "types"
  ],
  "files": [
    "/index.ts",
    "/index.d.ts",
    "/index.js",
    "/index.js.map",
    "/README.md",
    "/LICENSE"
